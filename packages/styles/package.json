{
    "name": "@brito/styles",
    "version": "1.0.1",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npm run format && npm run lint && rm -rf /dist && npm run build:esbuild",
        "build:esm": "tsc",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs",
        "build:esbuild": "node esbuild.js && tsc --emitDeclarationOnly --outDir dist/esm && tsc --emitDeclarationOnly --outDir dist/cjs",
        "dev": "rm -rf /dist && node esbuild.js && tsc --emitDeclarationOnly --outDir dist/esm && tsc --emitDeclarationOnly --outDir dist/cjs",
        "format": "prettier -w src",
        "lint": "eslint --fix src/**/*.ts*"
    },
    "license": "ISC",
    "dependencies": {
        "autoprefixer": "^10.4.13",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^3.0.0",
        "typescript": "4.9.4"
    },
    "devDependencies": {
        "@babel/core": "^7.20.7",
        "@typescript-eslint/eslint-plugin": "^5.48.0",
        "@typescript-eslint/parser": "^5.48.0",
        "babel-loader": "^9.1.0",
        "esbuild": "^0.16.13",
        "eslint": "^8.31.0",
        "eslint-config-standard-with-typescript": "^26.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.6.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-promise": "^6.0.0"
    }
}
